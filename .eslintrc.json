{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier",
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js"]
          }
        }
      },
    "rules": {
    }
}
